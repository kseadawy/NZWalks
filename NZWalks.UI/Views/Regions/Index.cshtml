@model IEnumerable<RegionDto>

<h1>Regions</h1>

<table class="table table-bordered">
	<thead>
	<tr>
		<th>ID</th>
		<th>Code</th>
		<th>Name</th>
	</tr>
	</thead>
	<tbody>
		@foreach (var region in Model)
		{
			<tr>
				<td>@region.Id</td>
				<td>@region.Code</td>
				<td>@region.Name</td>
			</tr>
		}
	</tbody>
</table>